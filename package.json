{
  "name": "@kero76/gh-actions-tests",
  "version": "0.1.0",
  "description": "A repository to test powerful of Github Actions",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "mocha -r ts-node/register ./tests/**/*.ts",
    "coverage": "nyc npm run test",
